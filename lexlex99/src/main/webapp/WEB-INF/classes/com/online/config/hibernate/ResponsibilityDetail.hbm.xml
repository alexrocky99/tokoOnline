<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.online.module.master.responsibility.model">
	<class name="ResponsibilityDetail" table="mst_responsibility_detail">
		<id name="responsibilityDetailId" column="responsibility_detail_id" type="long">
			<generator class="native" />
		</id>
		
		<many-to-one name="responsibility" column="responsibility_id"
			class="com.online.module.master.responsibility.model.Responsibility" fetch="join" />
		<many-to-one name="menu" column="menu_id"
			class="com.online.module.master.menu.model.Menu" fetch="join" />
		
		<property name="createBy" column="create_by" type="string" />
		<property name="createOn" column="create_on" type="timestamp" />
		<property name="updateBy" column="update_by" type="string" />
		<property name="updateOn" column="update_on" type="timestamp" />
		<property name="recordFlag" column="record_flag" type="string" />
	</class>
</hibernate-mapping>