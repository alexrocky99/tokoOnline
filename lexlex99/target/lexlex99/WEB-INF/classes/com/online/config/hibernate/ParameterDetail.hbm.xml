<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.online.module.master.parameter.model">
	<class name="ParameterDetail" table="mst_parameter_detail">
		<id name="parameterDetailId" column="parameter_detail_id" type="long">
			<generator class="native" />
		</id>
		
		<many-to-one name="parameter" column="parameter_id"
			class="com.online.module.master.parameter.model.Parameter" fetch="join" />
		
		<property name="detailCode" column="detail_code" type="string" />
		<property name="detailValue" column="detail_value" type="string" />
		<property name="detailDescription" column="detail_description" type="string" />
		<property name="status" column="status" type="string" />
		<property name="code" column="code" type="string" />
		
		<property name="createBy" column="create_by" type="string" />
		<property name="createOn" column="create_on" type="timestamp" />
		<property name="updateBy" column="update_by" type="string" />
		<property name="updateOn" column="update_on" type="timestamp" />
		<property name="recordFlag" column="record_flag" type="string" />
	</class>
</hibernate-mapping>